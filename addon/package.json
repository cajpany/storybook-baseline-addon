{
  "name": "storybook-addon-baseline",
  "version": "0.1.0",
  "description": "Storybook addon for displaying Baseline compatibility metadata",
  "keywords": [
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/storybook-addon-kit"
  },
  "type": "module",
  "license": "MIT",
  "author": "Baseline Hackathon Team",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "prestorybook": "npm run build",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "web-features": "^3.3.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^9.1.10",
    "tsup": "^8.2.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "storybook": "^9.1.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Addon Kit",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
